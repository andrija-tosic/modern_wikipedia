{
    "name": "Margin setter",
    "description": "Margin setter",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [{
        "matches": ["https://*.wikipedia.org/*"],
        "js": ["jquery-3.6.0.min.js", "math_equations.js"],
        "css": ["wikipedia.css"],
        "run_at": "document_end"
    },
    {
        "matches": ["https://sr.wikipedia.org/*"],
        "js": ["jquery-3.6.0.min.js", "math_equations.js"],
        "css": ["sr_wikipedia.css"],
        "run_at": "document_end"
    },
    {
        "css": ["font.css"],
        "matches": ["https://*/*"]
    }]
  }